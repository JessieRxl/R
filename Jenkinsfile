pipeline {
  agent any
  stages {
    stage('stage1') {
      parallel {
        stage('stage1') {
          steps {
            sh 'xcvxcvxcvxcv'
            echo 'sdfsdf'
          }
        }

        stage('stage2') {
          steps {
            sleep 33
          }
        }

      }
    }

  }
}