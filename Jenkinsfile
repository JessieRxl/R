pipeline {
  agent any
  stages {
    stage('error') {
      parallel {
        stage('errordddd') {
          steps {
            sh '55555'
            sh 'xcvcv'
          }
        }
        stage('') {
          steps {
            mail(subject: 'xcvxcv', body: 'xcvxcvxcv')
          }
        }
      }
    }
    stage('dfdfd') {
      steps {
        timestamps() {
          mail(subject: 'fgdfg', body: 'dfgdfg')
        }

        mail(subject: 'fgdfg', body: 'dfgdfg')
      }
    }
  }
}